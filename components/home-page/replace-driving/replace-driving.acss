image{
    width: 20rpx;
    height: 20rpx;
}
.rd{
    width: 94%;
    height: 790rpx;
    margin-left: 3%;
    background-color: #ffffff;
}
.rd-head-top{
    height: 90rpx;
    position: relative;
    overflow: hidden;  
}

.bg-right{
    height: 90rpx;
    border: 90rpx solid #FEDD00; 
    width: 236.5rpx;
    /* border-bottom: 100px solid red;
    border-left: 50px solid transparent;*/
    border-right: 50rpx solid transparent; 
    position: absolute;
}
.rd-head-top>view:nth-child(3){
    display: inline-block;
    position: absolute;
    z-index: 1;
    width: 50%;
    left: 0;
    border: 1rpx solid #b2ff22;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    color: #333333;
    font-size: 26rpx;
}
.rd-head-top>view:nth-child(4){
    position: absolute;
    z-index: 1;
    border: 1rpx solid #b2ff22;
    width: 50%;
    right:0; 
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    color: #333333;
    font-size: 26rpx;
}
