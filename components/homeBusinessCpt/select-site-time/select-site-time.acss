 .sst-bg{
    width: 100%;position: relative;
    background-color: rgb(255, 255, 255);
    border: 1rpx solid #ff0000;
    z-index: 11;
}
.sst-body {
    width: 94%;
    margin-left: 3%;
}

.sst-body-head {
    width: 100%;
    border-bottom: 1rpx solid #FAF9F9;
}

.sst-body-head>view {
}

.sst-body-head>view:nth-child(1) {
    height: 110rpx;
    position: relative;
    width: 176rpx;
    float: left;
}

.sst-body-head>view:nth-child(1)>view {
    font-size: 24rpx;
    color: #999999;
    position: absolute;
    left: 30rpx;
    top: 18rpx;
}

.sst-body-head>view:nth-child(1)>text {
    display: inline-block;
    font-size: 32rpx;
    color: #333333;
    font-family: Medium;
    margin-top: 66rpx;
    margin-left: 30rpx;
}

.sst-body-head>view:nth-child(2) {
    height: 110rpx;
    position: relative;
    float: left;
    width: 388rpx;
}

.sst-body-head>view:nth-child(2)>view {
    font-size: 24rpx;
    color: #999999;
    position: absolute;
    top: 18rpx;
}

.sst-body-head>view:nth-child(2)>text {
    display: inline-block;
    font-size: 32rpx;
    color: #333333;
    font-family: Medium;
    margin-top: 66rpx;
}

.sst-body-head>view:nth-child(3) {
    height: 110rpx;
    position: relative;
}

.sst-body-head>view:nth-child(3)>view {
    font-size: 24rpx;
    color: #999999;
    margin-top: 18rpx;
    float: right;
    margin-right: 32rpx
}

.sst-body-head>view:nth-child(3)>#select{
    float: right;
    margin-right: 32rpx;
    margin-top: 14rpx;
    width: 66rpx;
    height: 40rpx;
}
