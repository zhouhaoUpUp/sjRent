.sst-body-rump {
    height: 154rpx;
    position: relative;
}

.sst-body-rump>view:nth-child(1) {
    float: left;
    height: 154rpx;
    margin-left: 30rpx;
}

.sst-body-rump>view:nth-child(1)>view:nth-child(1) {
    color: #999999;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.sst-body-rump>view:nth-child(1)>view:nth-child(2) {
    color: #333333;
    font-size: 36rpx;
    margin-top: 12rpx;
}

.sst-body-rump>view:nth-child(1)>view:nth-child(3) {
    color: #999999;
    font-size: 28rpx;
}



.sst-body-rump>view:nth-child(3) {
    position: absolute;
    float: right;
    height: 154rpx;
    right: 30rpx;

}

.sst-body-rump>view:nth-child(3)>view:nth-child(1) {
    color: #999999;
    font-size: 24rpx;
    margin-top: 20rpx;
    text-align: right;
}

.sst-body-rump>view:nth-child(3)>view:nth-child(2) {
    color: #333333;
    font-size: 36rpx;
    margin-top: 12rpx;
    text-align: right;
}

.sst-body-rump>view:nth-child(3)>view:nth-child(3) {
    color: #999999;
    font-size: 28rpx;
    text-align: right;
}


.sst-body-rump>view:nth-child(2) {
    height: 100rpx;
    width: 100rpx;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
}
.sst-body-rump>view:nth-child(2)>text{
    font-size: 28rpx;
    color: #999999;
    width: 46rpx;
    height: 40rpx;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
}
.sst-body-rump>view:nth-child(2)>image{
    height: 14rpx;
    width: 96rpx;
    margin-top: 70rpx
}
.sst-rump{
    height: 32rpx;
}
.sst-rump>image{
    width: 22rpx;
    height: 22rpx;
    float: left;
    margin-top: 2rpx;
    margin-left: 6rpx;
}
.sst-rump>view{
        float: left;
    font-size: 22rpx;
    color: #999999;
    margin-left: 6rpx;
}
button{
    background-color: #FEDD00;
    border: 0;
    color: #333333;
    margin-bottom: 30rpx;
}
.sst-changeSize{
    transform: scale(0.7);
    padding: 0;
    width: 66rpx;
    height: 40rpx
}
