<!-- 选择车辆类型组件 -->
<select-car-class a:if="{{carClassState}}" onCarClassShow="toCarClassShow"/>
<!-- 座位数组件 -->
<select-seating a:if="{{seatAmountState}}" onSeatingShow="toSeatingShow"/>
<!-- 供应商选择 -->
<select-supplier a:if="{{supplyState}}" onSupplier="toSupplier"/>
<!-- 排序组件 -->
<select-sort a:if="{{filtrSteta}}" onSort="toSort"/>