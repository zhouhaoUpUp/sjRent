<!-- 选择时间组件 -->
<select-site-time/>
<view>
  <view class="top">
    <view>
    西部智谷D区
    </view>
    <view>
    11月27日
    </view>
    <view>
    18:50
    </view>
    <view>
    2天
    <image mode="scaleToFill" src="../../../assets/carList/Calendar_sele@2x.png"/>
    </view>
  </view>
  
  <view class="head">
    <view onTap="carClassPopUp">
      车辆类型
